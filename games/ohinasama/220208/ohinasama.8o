:alias py v2

: main
	hires
  px := 25
  py := 24
	plane 1
	i := bonbori1
	sprite px py 0
	plane 2
	i := bonbori2
	sprite px py 0

  px := 45
  py := 20
	plane 1
	i := odairi1
	sprite px py 0
	plane 2
	i := odairi2
	sprite px py 0

  px := 67
  py := 20
	plane 1
	i := ohinasama1
	sprite px py 0
	plane 2
	i := ohinasama2
	sprite px py 0

  px := 88
  py := 24
	plane 1
	i := bonbori1
	sprite px py 0
	plane 2
	i := bonbori2
	sprite px py 0

  px := 40
  py := 36
	loop
	  plane 1
	  i := daiza1
	  sprite px py 4
	  plane 2
	  i := daiza2
	  sprite px py 4
    px += 8
    if px != 88 then
	again
	
	loop
	again

